<IfModule mod_rewrite.c>
   RewriteEngine on

   RewriteCond %{HTTPS} =off 
   RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [QSA,L]

   RewriteCond %{HTTP_HOST} ^www\.(.*)$
   RewriteRule ^(.*)$ http://%1/$1 [L,R=301]

   RewriteRule    ^$ app/webroot/    [L]
   RewriteRule    (.*) app/webroot/$1 [L]
</IfModule>